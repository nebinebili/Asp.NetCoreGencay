<h3>ViewBag</h3>
<ul>

    @foreach (var product in ViewBag.Mehsullar)
    {
        <li>@product.ProductName</li>
    }

</ul>

<h3>ViewData</h3>
<ul>

    @foreach (var product in ViewData["Mehsullar"] as List<GencayExample.Models.Product>)
    {
        <li>@product.ProductName</li>
    }

</ul>

<h3>TempData</h3>
<ul>

    @foreach (var product in TempData["Mehsullar"] as List<GencayExample.Models.Product>)
    {
        <li>@product.ProductName</li>
    }

</ul>